generator client {
  provider = "prisma-client-js"
 
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model ContactForm {
  id        Int     @id @default(autoincrement())
  email     String   @unique
  name      String
  message   String
  createdAt DateTime @default(now())
}
