datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  directUrl         = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ContactForm {
  id        Int     @id @default(autoincrement())
  email     String   @unique
  Name      String
  Message   String
  createdAt DateTime @default(now())
}
